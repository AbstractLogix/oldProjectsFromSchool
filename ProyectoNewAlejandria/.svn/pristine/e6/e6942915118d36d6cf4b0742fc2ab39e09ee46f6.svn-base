<div class="container-fluid">
<div class="row" ng-controller="TableEmbarque">
		<div class="col-md-7">
			<div class="row">
				<div class="col-md-6">
					<select class="form-control" ng-model="searchText" ng-change=Filtro()>
                                            <option>Todos</option>
                                            <option>Aerea</option>
                                            <option>Courier</option>
                                            <option>Box</option>
                                            <option>LCL</option>
                                            <option>FCL</option>
                                        </select>
				</div>
				<div class="col-md-6">
                                    <button class="ghostb" ng-click="Exportar()"> Exportar </button>         
				</div>
			</div>
                        <br>
			<div class="row">
				<div class="col-md-12">
                                    <small>
                                    <div>
                                        <font size="2" align="center">
                                        <div id="grid1" ui-grid="gridOptions" class="grid"  ng-click=selectRow() ui-grid-selection></div>
                                        </font>
                                    </div>
                                    </small>
				</div>
			</div>
			<div class="row">
				<div class="col-md-6">
                                    Piesas: {{Piezas}}
                                    <br>
                                    Peso Vol: {{PesoV}} {{Medida}}
				</div>
				<div class="col-md-6">
                                    Peso: {{Peso}}  {{Medida}}
                                    <br>
                                    Cft: {{Ctf}}
                                    {{gridOptions.selectedItems}}
				</div>
			</div>
		</div>
		<div class="col-md-5">
                    <div class="row">
                        <div class="col-md-12">
                              Warehouses del documento
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                           <small>
                                <div>
                                    <font size="2" align="center">
                                    <div id="grid1" ui-grid="gridOptions2" class="gridStyleD" ui-grid-selection></div>
                                    </font>
                                </div>
                           </small>
                        </div>
                    </div>
                    <br>
                    <div class="row">
                        <div class="col-md-12">
                            <font size="1.8">
                              <table style="width:100%;">
                                  <tr>
                                    <td><div ng-repeat = "VF in TF track by $index">{{VF}}</div></td>
                                    <td><div ng-repeat = "VM in TM track by $index">{{VM}}</div></td>
                                 </tr>
                              </table>     
                           </font>
                        </div>
                    </div>
                    
		</div>
</div>
    </div>
