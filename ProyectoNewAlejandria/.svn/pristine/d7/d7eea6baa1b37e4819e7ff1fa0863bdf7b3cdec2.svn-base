<div class="container-fluid">
<div class="row">
 
    <div class="col-md-12">
        <div class="ui-tabs"> 
            <ul class="ui-tabs-nav">
                <a href="#TabPerfil" >Mi Perfil</a>
                <a href="#TabConf">Configuracion</a>
                <a href="#TabContactos" selected>Contactos</a>
                <a href="#TabCContraseña">Cambiar Contraseña</a>
            </ul>
        </div>
    </div>
    <div class="col-sm-6">
        <div id="grid1" ui-grid="gridOptions" class="grid" Style="height: 400px; width: 100%;"  ng-click=selectRow() ui-grid-selection></div>
    </div>
    <div class="col-sm-6">
        <table>
        <tr>
            <td><label for="surname">Prefijo</label></td>
            <td><input class="Docu" ng-model="Direccion" type="text"></td>
        </tr>
        <tr>
            <td><label for="surname">Nombres</label></td>
            <td><input class="Docu" ng-model="Nombres" type="text"></td>
        </tr>
        <tr>
            <td><label for="surname">Apellidos</label></td>
            <td><input class="Docu"  ng-model="Apellidos" type="text"></td>
        </tr>
        <tr>
            <td><label for="surname">Puesto</label></td>
            <td><input class="Docu" ng-model="Puesto" type="text"></td>
        </tr>
        <tr>
            <td><label for="surname">Dirección</label></td>
            <td><input class="Docu"  ng-model="Direccion" type="text"></td>
        </tr>
        <tr>
            <td><label for="surname">Teléfono</label></td>
            <td><input class="Docu" ng-model="Tel" type="text"></td>
        </tr>
       <tr>
            <td><label for="surname">Cellphone</label></td>
            <td><input class="Docu" ng-model="Cel" type="text"></td>
        </tr>
        <tr>
            <td><label for="surname">Fax</label></td>
            <td><input class="Docu" ng-model="Fax" type="text"></td>
        </tr>
       <tr>
            <td><label for="surname">Email</label></td>
            <td><input class="Docu" ng-model="Email" type="text"></td>
        </tr>
        <tr>
            <td colspan="2">
            <button type="submit" style="font-size:100%;">Actualizar</button>
            <button type="submit" style="font-size:100%;">Eliminar</button>
            <button type="submit" style="font-size:100%;">Limpiar</button>
            </td>
        </tr>
        </table>
        
    </div>
</div>
</div>